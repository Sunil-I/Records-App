<script>
    new Chart(document.getElementById("transactionsPerYear"), {
      type: "line",
      data: {
        labels: [<%= Object.keys(yearlyTransactions) %>],
        datasets: [
          {
            label: "Transactions",
            borderColor: 'rgb(75, 192, 192)',
            data:[<%= Object.values(yearlyTransactions) %>],
          },
        ],
      },
      options: {
        legend: { display: false },
        title: {
          display: true,
          text: "Transactions per Year",
        },
        scales: {
          yAxes: [{
            scaleLabel: {
              display: true,
              labelString: 'Transactions made'
            },
            ticks: {
             beginAtZero: true
            },
          }]
        }
      },
    });
 </script>