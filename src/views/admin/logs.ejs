<!DOCTYPE html>
<html lang="en">
   <head>
      <meta charset="utf-8" />
      <meta
         name="viewport"
         content="width=device-width initial-scale=1 shrink-to-fit=no"
         />
      <meta name="description" content="data-driven full-stack web-application" />
      <title>Admin Dashboard - Login Logs</title>
      <!-- Bootstrap core CSS -->
      <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
      <link rel="stylesheet" href="/css/admin/dashboard.css">
      <!-- ajax -->
      <script src="https://code.jquery.com/jquery-3.6.0.min.js" defer> </script>
   </head>
   <body>
      <%- include('../common/header'); -%>
      <div class="container-fluid">
         <div class="row">
            <nav id="sidebarMenu" class="col-md-2 d-none d-md-block bg-light sidebar">
               <div class="sidebar-sticky">
                  <ul class="nav flex-column">
                     <a class="nav-link" href="/admin">
                        <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather">
                           <path d="M1 12s4-8 11-8 11 8 11 8-4 8-11 8-11-8-11-8z"></path>
                           <circle cx="12" cy="12" r="3"></circle>
                        </svg>
                        Dashboard
                     </a>
                     <a class="nav-link" href="/admin/transactions">
                        <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather">
                           <rect x="1" y="4" width="22" height="16" rx="2" ry="2"></rect>
                           <line x1="1" y1="10" x2="23" y2="10"></line>
                        </svg>
                        Transactions
                     </a>
                     <li class="nav-item">
                        <a class="nav-link" href="/admin/accounts">
                           <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather">
                              <line x1="12" y1="1" x2="12" y2="23"></line>
                              <path d="M17 5H9.5a3.5 3.5 0 0 0 0 7h5a3.5 3.5 0 0 1 0 7H6"></path>
                           </svg>
                           Accounts
                        </a>
                     </li>
                     <li class="nav-item">
                        <a class="nav-link" href="/admin/users">
                           <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather">
                              <path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path>
                              <circle cx="9" cy="7" r="4"></circle>
                              <path d="M23 21v-2a4 4 0 0 0-3-3.87"></path>
                              <path d="M16 3.13a4 4 0 0 1 0 7.75"></path>
                           </svg>
                           Users
                        </a>
                     </li>
                     <li class="nav-item">
                        <a class="nav-link" href="/admin/manage">
                           <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather">
                              <path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"></path>
                              <polyline points="7 10 12 15 17 10"></polyline>
                              <line x1="12" y1="15" x2="12" y2="3"></line>
                           </svg>
                           Manage Data
                        </a>
                     </li>
                     <li class="nav-item">
                        <a class="nav-link item" href="/admin/logs">
                           <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather">
                              <rect x="2" y="3" width="20" height="14" rx="2" ry="2"></rect>
                              <line x1="8" y1="21" x2="16" y2="21"></line>
                              <line x1="12" y1="17" x2="12" y2="21"></line>
                           </svg>
                           Login Logs <span class="sr-only">(current)</span>
                        </a>
                     </li>
                  </ul>
                  </ul>
               </div>
            </nav>
            <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">

                <h1 class="mx-auto text-center h2">Login Logs</h1>
      
                <table class="table table-striped">
                   <thead>
                      <tr>
                        <th>Log ID</th>
                        <th>User Email</th>
                        <th>Status</th>
                        <th>Reason</th>
                        <th>Date</th>
                    </tr>
                      <%  
                         logs.forEach(log => {
                           
                         %>
                      <tr>
                        <td><%=  log.log_id  %></td>
                        <td><%=  log.user_email  %></td>
                        <td><%=  log.status  %></td>
                        <td><%=  log.reason  %></td>
                        <td><%=  log.createdAt.toLocaleDateString('en-GB') %></td>
                      </tr>
                      <%  
                         });
                         %>
                   </thead>
                </table>
            </main>
         </div>
      </div>
      <%- include("../common/pagination") -%>
      <%- include("../common/footer") -%>
   </body>
</html>